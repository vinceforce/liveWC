<html>
<head>
  <meta charset="UTF-8">
  <title>Visualisation vid√©o 1</title>
  <script src="https://code.jquery.com/jquery-1.11.2.min.js"></script>
  <script type="text/javascript" src="/socket.io/socket.io.js"></script>
</head>
<body>
  <img id="play-1" />
  <img id="play-2" />
  <img id="play-3" />
  <img id="play-4" />
  <img id="play-5" />
  <img id="play-6" />
  <img id="play-7" />
  <img id="play-8" />
  <img id="play-9" />
  <img id="play-10" />
  <div id="logger"></div>
  <script>
    var socket = io('/<%=channel%>');
    for (var ii=1; ii<=6; ii++) {
      socket.on('stream-'+ii, function(image){
        var img = document.getElementById("play-" + ii);
        img.src = image;
        //$("#logger").text(image);
      });
    }
  </script>
</body>
</html>
